{
	"highlight.regexes": {
		"(##[^#]*##)": {
			"filterFileRegex": ".*\\.tex$",
			"decorations": [
				{
					"color": "#53d33f",
					"fontWeight": "bold",
					// "textDecoration": "underline"
				}
			]
		},
		"(<<[^<>]*>>)": {
			"filterFileRegex": ".*\\.tex$",
			"decorations": [
				{
					"color": "#53d33f",
					"fontWeight": "bold",
					// "textDecoration": "underline"
				}
			]
		}
	},
	"highlight.decorations": {
		"rangeBehavior": 3
	}
}